{
    "stage": 0,
    "optional": [
        "runtime"
    ],
    "ignore": [
        "d3.js"
    ],
    "env": {
        "development": {
            "plugins": [
                "react-transform"
            ],
            "extra": {
                "react-transform": {
                    "transforms": [{
                        "transform": "react-transform-hmr",
                        "imports":   ["react"],
                        "locals":    ["module"]
                    }]
                }
            }
        }
    }
}
